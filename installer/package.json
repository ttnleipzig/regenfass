{
	"name": "regenfass-www",
	"version": "1.0.0",
	"description": "Install regenfass with your browser",
	"private": true,
	"scripts": {
		"build": "postcss ./public/css/_style.css -o ./public/css/style.css",
		"css:dev": "pnpm run build --watch --verbose",
		"dev": "concurrently -n 'css,http' 'pnpm run css:dev' 'pnpm run start'",
		"start": "serve .",
		"test": "pnpm exec playwright test"
	},
	"keywords": [],
	"author": "",
	"packageManager": "pnpm@9.1.1",
	"license": "AGPL-3.0-only",
	"devDependencies": {
		"autoprefixer": "^10.4.19",
		"concurrently": "^8.2.2",
		"cssnano": "^6.1.2",
		"postcss": "^8.4.38",
		"postcss-cli": "^11.0.0",
		"serve": "^14.2.3",
		"tailwindcss": "^3.4.3"
	},
	"dependencies": {
		"@playwright/test": "^1.43.1",
		"@types/node": "^20.12.8"
	}
}
